                 <a href="login.jsp"><button class="btn btn-outline-primary b1"><b> Login  </b></button></a>

                <form action="registration" method="post" name="myform">
                <h2 class="text-uppercase text-center mb-5">Registration Form</h2>
                 <div class="form-outline ">
                 <label class="form-label" for="form3Example1cg">First Name</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="firstName" required/>
                 </div>

                 <div class="form-outline">
                 <label class="form-label" for="form3Example3cg">Last Name </label>
                 <input type="text" id="form3Example3cg" class="form-control form-control" name="lastName" required/>
                 </div>

                 <div class="form-outline"
                 <label class="form-label" for="form3Example1cg">Address</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="address"/>
                 </div>

                 <div class="form-outline">
                 <label class="form-label" for="form3Example1cg">City</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="city" />
                 </div>

                 <div class="form-outline"
                 <label class="form-label" for="form3Example1cg">State</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="state"/>
                 </div>

                 <div class="form-outline">
                 <label class="form-label" for="form3Example1cg">Country</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="country" />
                 </div>

                 <div class="form-outline">
                 <label class="form-label" for="form3Example1cg">Zip</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="zip"/>
                 </div>

                 <div class="form-outline">
                 <label class="form-label" for="form3Example1cg">Phone</label>
                 <input type="text" id="form3Example1cg" class="form-control form-control" name="phone"/>
                 </div>

                  <div class="form-outline">
                  <label class="form-label" for="form3Example1cg">UserEmailId</label>
                  <input type="email" id="form3Example1cg" class="form-control form-control" name="userId"/>
                  </div>

                  <div class="form-outline">
                  <label class="form-label" for="form3Example1cg">Password</label>
                  <input type="password" id="form3Example1cg" class="form-control form-control" name="password"/>
                  </div>

                   <br>
                 <div class="d-flex justify-content-center">
                 <button type="submit" class="btn btn-success btn-block btn-lg gradient-custom-4 text-body">Register</button>
